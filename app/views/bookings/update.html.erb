<h1>Bookings#update</h1>

<%= form_with model: @booking do |f| %>
  <%= f.label :entry_date %>
  <%= f.text_field :entry_date %>
  <%= f.label :out_date %>
  <%= f.text_field :out_date %>
  <%= f.label :guest %>
  <%= f.text_field :guest %>
  <%= f.submit %>
<% end %>

<p><%= link_to '< Back', bookings_path %></p>
